! RXVT Config. terminus font is required!
! sudo update-alternatives --set x-terminal-emulator "$(which terminator)"

URxvt.font: xft:Terminus:size=13

URxvt*transparent: true
URxvt*tintColor: White
URxvt*shading: 40
URxvt*scrollBar: false
URxvt.internalBorder: 20

! terminal.sexy
! tango on hybrid

! special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6

! black
*.color0:       #2e3436
*.color8:       #555753

! red
*.color1:       #cc0000
*.color9:       #ef2929

! green
*.color2:       #4e9a06
*.color10:      #8ae234

! yellow
*.color3:       #c4a000
*.color11:      #fce94f

! blue
*.color4:       #3465a4
*.color12:      #729fcf

! magenta
*.color5:       #75507b
*.color13:      #ad7fa8

! cyan
*.color6:       #06989a
*.color14:      #34e2e2

! white
*.color7:       #707070
*.color15:      #c5c8c6
